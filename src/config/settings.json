{
  "http": {
    "timeout": 20,
    "max_retries": 3,
    "backoff_factor": 0.7,
    "user_agent": "KauflandFastProductScraper/1.0 (+https://bitbash.dev)"
  },
  "scraper": {
    "concurrency": 5,
    "sleep_between_requests": 0.15
  },
  "output": {
    "directory": "data",
    "filename_prefix": "kaufland_products"
  },
  "logging": {
    "level": "INFO"
  }
}